      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0042-4900</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>107</Volume>
            <Issue>9</Issue>
            <PubDate>
              <Year>1980</Year>
              <Month>Aug</Month>
              <Day>30</Day>
            </PubDate>

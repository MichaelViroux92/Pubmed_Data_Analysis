      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0017-5749</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>21</Volume>
            <Issue>4</Issue>
            <PubDate>
              <Year>1980</Year>
              <Month>Apr</Month>

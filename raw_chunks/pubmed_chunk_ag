  <PubmedArticle>
    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">4776883</PMID>
      <DateCompleted>
        <Year>1974</Year>
        <Month>04</Month>
        <Day>03</Day>
      </DateCompleted>
      <DateRevised>
        <Year>2019</Year>

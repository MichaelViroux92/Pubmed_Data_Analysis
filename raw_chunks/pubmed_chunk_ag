  <PubmedArticle>
    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">6271008</PMID>
      <DateCompleted>
        <Year>1981</Year>
        <Month>12</Month>

    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">6107676</PMID>
      <DateCompleted>
        <Year>1981</Year>
        <Month>01</Month>
        <Day>29</Day>
      </DateCompleted>
      <DateRevised>
        <Year>2019</Year>

            <Initials>KE</Initials>
          </Author>
          <Author ValidYN="Y">
            <LastName>Johnson</LastName>
            <ForeName>C A</ForeName>
            <Initials>CA</Initials>
          </Author>
        </AuthorList>
        <Language>eng</Language>

      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0026-0495</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>34</Volume>
            <Issue>8</Issue>
            <PubDate>
              <Year>1985</Year>
              <Month>Aug</Month>

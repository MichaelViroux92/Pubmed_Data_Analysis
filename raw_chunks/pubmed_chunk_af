      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0015-329X</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>71</Volume>
            <Issue>2</Issue>
            <PubDate>
              <Year>1985</Year>
              <Month>Feb</Month>

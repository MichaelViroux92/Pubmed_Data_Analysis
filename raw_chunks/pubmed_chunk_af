    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">3763817</PMID>
      <DateCompleted>
        <Year>1986</Year>
        <Month>11</Month>
        <Day>10</Day>

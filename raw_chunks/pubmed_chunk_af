      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0300-8924</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>7</Volume>
            <Issue>3-4</Issue>
            <PubDate>
              <Year>1985</Year>

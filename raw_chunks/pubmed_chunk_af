    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">3881031</PMID>
      <DateCompleted>
        <Year>1985</Year>
        <Month>02</Month>
        <Day>11</Day>

      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0317-1671</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>14</Volume>
            <Issue>4</Issue>
            <PubDate>
              <Year>1987</Year>

  <PubmedArticle>
    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">3841460</PMID>
      <DateCompleted>
        <Year>1986</Year>
        <Month>02</Month>

      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0043-5341</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>137</Volume>
            <Issue>14-15</Issue>
            <PubDate>
              <Year>1987</Year>

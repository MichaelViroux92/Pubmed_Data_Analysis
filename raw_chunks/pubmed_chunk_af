      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0264-6021</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>231</Volume>
            <Issue>1</Issue>
            <PubDate>
              <Year>1985</Year>
              <Month>Oct</Month>
              <Day>01</Day>
            </PubDate>

      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0378-7346</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>24</Volume>
            <Issue>1</Issue>
            <PubDate>
              <Year>1987</Year>

  <PubmedArticle>
    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">3776496</PMID>
      <DateCompleted>
        <Year>1986</Year>
        <Month>12</Month>
        <Day>04</Day>

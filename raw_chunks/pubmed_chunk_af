      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0066-2070</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>22</Volume>
            <Issue>5</Issue>
            <PubDate>
              <Year>1986</Year>

      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0305-1048</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>13</Volume>
            <Issue>24</Issue>
            <PubDate>
              <Year>1985</Year>
              <Month>Dec</Month>

    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">4027624</PMID>
      <DateCompleted>
        <Year>1985</Year>
        <Month>10</Month>
        <Day>23</Day>
      </DateCompleted>
      <DateRevised>
        <Year>2019</Year>

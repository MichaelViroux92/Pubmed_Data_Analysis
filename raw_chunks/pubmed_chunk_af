      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0096-1736</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>27</Volume>
            <Issue>10</Issue>
            <PubDate>
              <Year>1985</Year>

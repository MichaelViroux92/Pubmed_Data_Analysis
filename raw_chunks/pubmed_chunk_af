    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">3928054</PMID>
      <DateCompleted>
        <Year>1985</Year>
        <Month>10</Month>
        <Day>24</Day>
      </DateCompleted>
      <DateRevised>
        <Year>2019</Year>

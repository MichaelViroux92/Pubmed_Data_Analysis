      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0038-4348</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>78</Volume>
            <Issue>6</Issue>
            <PubDate>
              <Year>1985</Year>

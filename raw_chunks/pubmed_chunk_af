    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">3838732</PMID>
      <DateCompleted>
        <Year>1985</Year>
        <Month>06</Month>
        <Day>07</Day>
      </DateCompleted>

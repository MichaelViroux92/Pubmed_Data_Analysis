      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0036-7672</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>116</Volume>
            <Issue>10</Issue>
            <PubDate>
              <Year>1986</Year>
              <Month>Mar</Month>
              <Day>08</Day>

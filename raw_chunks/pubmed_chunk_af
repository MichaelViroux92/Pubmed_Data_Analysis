      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0248-8663</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>6</Volume>
            <Issue>5</Issue>
            <PubDate>
              <Year>1985</Year>

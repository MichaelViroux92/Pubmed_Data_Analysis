      </DateCompleted>
      <DateRevised>
        <Year>2019</Year>
        <Month>06</Month>
        <Day>19</Day>
      </DateRevised>
      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0008-543X</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>55</Volume>
            <Issue>8</Issue>
            <PubDate>
              <Year>1985</Year>
              <Month>Apr</Month>
              <Day>15</Day>

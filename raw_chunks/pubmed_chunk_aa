      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0002-9343</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>63</Volume>
            <Issue>2</Issue>
            <PubDate>
              <Year>1977</Year>

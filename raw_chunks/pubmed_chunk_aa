    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">60515</PMID>
      <DateCompleted>
        <Year>1976</Year>
        <Month>11</Month>
        <Day>01</Day>
      </DateCompleted>
      <DateRevised>
        <Year>2019</Year>

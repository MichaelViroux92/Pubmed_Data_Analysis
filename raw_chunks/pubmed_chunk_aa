  <PubmedArticle>
    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">309283</PMID>
      <DateCompleted>
        <Year>1978</Year>
        <Month>12</Month>
        <Day>27</Day>
      </DateCompleted>
      <DateRevised>
        <Year>2019</Year>

    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">117883</PMID>
      <DateCompleted>
        <Year>1980</Year>
        <Month>03</Month>
        <Day>27</Day>
      </DateCompleted>
      <DateRevised>
        <Year>2019</Year>
        <Month>08</Month>

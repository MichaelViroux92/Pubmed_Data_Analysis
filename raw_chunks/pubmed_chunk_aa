  <PubmedArticle>
    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">221630</PMID>
      <DateCompleted>
        <Year>1979</Year>
        <Month>08</Month>
        <Day>16</Day>
      </DateCompleted>
      <DateRevised>
        <Year>2022</Year>

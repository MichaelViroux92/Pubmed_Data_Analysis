      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0002-9629</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>276</Volume>
            <Issue>3</Issue>
            <PubDate>
              <Year>1978</Year>

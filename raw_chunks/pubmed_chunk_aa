      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0027-8874</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>62</Volume>
            <Issue>2</Issue>
            <PubDate>
              <Year>1979</Year>

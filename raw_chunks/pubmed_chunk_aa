    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">23950</PMID>
      <DateCompleted>
        <Year>1978</Year>
        <Month>04</Month>
        <Day>26</Day>

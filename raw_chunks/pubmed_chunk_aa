    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">389483</PMID>
      <DateCompleted>
        <Year>1980</Year>
        <Month>02</Month>
        <Day>26</Day>

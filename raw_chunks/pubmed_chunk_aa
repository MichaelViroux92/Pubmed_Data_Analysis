      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0023-6764</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>28</Volume>
            <Issue>4</Issue>
            <PubDate>
              <Year>1978</Year>

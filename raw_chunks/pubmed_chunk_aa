      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0042-8809</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>23</Volume>
            <Issue>4</Issue>
            <PubDate>
              <Year>1977</Year>

      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">1660-8151</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>15</Volume>
            <Issue>2</Issue>
            <PubDate>
              <Year>1975</Year>

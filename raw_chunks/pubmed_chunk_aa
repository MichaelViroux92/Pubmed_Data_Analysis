      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0037-9026</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>171</Volume>
            <Issue>3</Issue>
            <PubDate>
              <Year>1977</Year>

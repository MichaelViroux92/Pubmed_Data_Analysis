    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">73853</PMID>
      <DateCompleted>
        <Year>1978</Year>
        <Month>02</Month>
        <Day>18</Day>
      </DateCompleted>
      <DateRevised>

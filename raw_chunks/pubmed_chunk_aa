    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">206262</PMID>
      <DateCompleted>
        <Year>1978</Year>
        <Month>06</Month>
        <Day>12</Day>
      </DateCompleted>
      <DateRevised>
        <Year>2019</Year>

    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">218643</PMID>
      <DateCompleted>
        <Year>1979</Year>
        <Month>06</Month>
        <Day>11</Day>
      </DateCompleted>

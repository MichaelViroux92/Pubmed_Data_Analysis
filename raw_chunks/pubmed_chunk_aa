      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0040-8727</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>129</Volume>
            <Issue>1</Issue>
            <PubDate>
              <Year>1979</Year>

    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">545839</PMID>
      <DateCompleted>
        <Year>1980</Year>
        <Month>07</Month>
        <Day>12</Day>

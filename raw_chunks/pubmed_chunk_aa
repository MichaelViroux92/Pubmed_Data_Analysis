      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0301-5661</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>5</Volume>
            <Issue>3</Issue>
            <PubDate>
              <Year>1977</Year>
              <Month>May</Month>

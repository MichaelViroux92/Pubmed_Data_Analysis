    <MedlineCitation Status="MEDLINE" Owner="NLM" IndexingMethod="Manual">
      <PMID Version="1">640344</PMID>
      <DateCompleted>
        <Year>1978</Year>
        <Month>06</Month>
        <Day>17</Day>

      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0004-0169</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>60</Volume>
            <Issue>1-4</Issue>
            <PubDate>
              <Year>1976</Year>
              <Season>Jan-Dec</Season>

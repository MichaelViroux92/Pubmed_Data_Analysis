      <Article PubModel="Print">
        <Journal>
          <ISSN IssnType="Print">0002-922X</ISSN>
          <JournalIssue CitedMedium="Print">
            <Volume>133</Volume>
            <Issue>9</Issue>
            <PubDate>
              <Year>1979</Year>
